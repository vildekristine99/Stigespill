package no.hvl.dat109;

import java.util.ArrayList;
import java.util.List;

/**
 * @author Vilde Kristine Fossum
 *
 */
public class Brett {
	private Rute[] ruter = new Rute[100];
	
	/**
	 * Finner og retunerer ruten som er sum steg frem for rute
	 * 
	 * @param rute
	 * @param sum
	 * @return
	 */
	public Rute finnRute(Rute rute, Integer verdi) {
		Rute nyRute = ruter.get(rute.getRuteNr() + verdi);
		return nyRute;
	}
	
}
